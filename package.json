{
  "name": "vitepress-blog",
  "version": "1.1.0",
  "description": "expand vitepress for ssg blog",
  "main": "./dist/node/index.js",
  "type": "module",
  "types": "./dist/node/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/node/index.d.ts",
      "require": "./dist/node/index.js",
      "import": "./dist/node/index.js"
    },
    "./theme": {
      "types": "./theme.d.ts",
      "default": "./dist/client/theme/index"
    },
    "./theme-helper": {
      "types": "./theme-helper.d.ts",
      "default": "./dist/client/index"
    },
    "./package.json": "./package.json"
  },
  "bin": "./bin/init.js",
  "scripts": {
    "dev": "rollup -c --bundleConfigAsCjs -w",
    "build": "rimraf dist && tsc && rollup -c --bundleConfigAsCjs",
    "link_pnpm": "pnpm link --global"
  },
  "files": [
    "dist",
    "package.json",
    "env.d.ts",
    "README.md",
    "theme.d.ts",
    "theme-helper.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yue1123/vitepress-blog"
  },
  "keywords": [
    "vitepress",
    "vitepress-blog",
    "blog",
    "SSG",
    "ssg",
    "vue-blog"
  ],
  "author": "yue1123",
  "packageManager": "pnpm@7.9.2",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-typescript": "^10.0.1",
    "@types/node": "^18.11.9",
    "rimraf": "^3.0.2",
    "rollup": "^3.9.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-dts": "^5.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-scss": "^4.0.0",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-typescript2": "^0.34.1",
    "rollup-plugin-vue": "^6.0.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.3"
  },
  "dependencies": {
    "bootstrap-icons-vue": "^1.8.1",
    "chalk": "^5.2.0",
    "commander": "^9.4.1",
    "deepmerge": "^4.2.2",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-image-lazy-loading": "^1.2.0",
    "markdown-it-imsize": "^2.0.1",
    "markdown-it-katex": "^2.0.3",
    "markdown-it-mathjax": "^2.0.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-task-lists": "^2.1.1",
    "markdown-it-textual-uml": "^0.12.0",
    "prompts": "^2.4.2",
    "sass": "^1.57.1",
    "vitepress": "1.0.0-alpha.33",
    "vue": "^3.2.45"
  }
}
